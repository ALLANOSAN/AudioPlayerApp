  "scripts": {
    "start": "expo start --no-dev-client",
    "start:dev-client": "expo start --dev-client",
    "start:auto": "node start-expo-auto.js",
    "start:non-interactive": "node start-expo-non-interactive.js",
    "ios": "expo run:ios",
    "android": "expo run:android",
    "build:dev": "eas build --profile development",
    "build:preview": "eas build --profile preview",
    "build:prod": "eas build --profile production",
    "prebuild": "expo prebuild",
    "lint": "eslint \"**/*.{js,jsx,ts,tsx}\" && prettier -c \"**/*.{js,jsx,ts,tsx,json}\"",
    "format": "eslint \"**/*.{js,jsx,ts,tsx}\" --fix && prettier \"**/*.{js,jsx,ts,tsx,json}\" --write",
    "web": "expo start --web",
    "test": "jest",
    "e2e:build:android": "detox build -c android.emu.debug",
    "e2e:test:android": "detox test -c android.emu.debug",
    "e2e:build:ios": "detox build -c ios.sim.debug",
    "e2e:test:ios": "detox test -c ios.sim.debug"
